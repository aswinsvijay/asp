{
  "version": "1.0.0",
  "platformVersion": "16.5.1",
  "authentication": {
    "type": "basic",
    "test": {
      "url": "https://{{process.env.HOST_NAME}}/api/000000000000000000000000/children"
    },
    "connectionLabel": "{{username}}"
  },
  "creates": {
    "GetChildren": {
      "noun": "GetChildren",
      "operation": {
        "perform": {
          "url": "https://{{process.env.HOST_NAME}}/api/{{bundle.inputData.parentId}}/children",
          "method": "GET",
          "headers": {
            "Content-Type": "application/json",
            "Accept": "*/*"
          },
          "body": "{{bundle.inputData}}",
          "removeMissingValuesFrom": {
            "body": false,
            "params": false
          }
        },
        "inputFields": [
          {
            "key": "parentId",
            "type": "string"
          }
        ],
        "outputFields": [
          {
            "key": "data__name",
            "type": "string"
          },
          {
            "key": "data__path",
            "type": "string"
          },
          {
            "choices": [
              "document",
              "folder"
            ],
            "key": "data__type",
            "type": "string"
          }
        ]
      },
      "display": {
        "description": "Very very very very long description for GetChildren",
        "hidden": false,
